class Node:
    def _init_(self,dataval = None):
        self.dataval = dataval
        self.nextval = None
class SLinkedlist:
    def _init_(self):
        self.headval = None
    def listprint(self):
        printval = self.headval
        while printval is not None:
            print(printval.dataval)
            printval = printval.nextval
    def at_end(self,newdata):
        newNode = Node(newdata)
        if self.headval is None:
            self.headval = newNode
            return
        last = self.headval
        while(last.nextval):
            last = last.nextval
        last.nextval = newNode
list = SLinkedlist()
list.headval = Node("Mon")
e2 = Node("Tue")
e3 = Node("Wed")
list.headval.nextval = e2
e2.nextval = e3
list.listprint()
list.at_end("Thurs")
list.listprint()
